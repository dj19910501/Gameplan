                dataType: "json",
                success: function (data) {
                    if (data == 'exist') {
                        var msgspan = "@Common.objCached.ModelAlreadyExits";
                        $("#errorMessagecopy").css("display", "block");
                        $("#spanErrorMessageModelcopy").text(msgspan);
