                    <h5>View By:</h5>
                    <div class="revenue-summary-custom">
                        @Html.DropDownList("ddlParentRevenueSummery", new SelectList((System.Collections.IEnumerable)ViewBag.parentRevenueSummery, "Value", "Text"))
                    </div>
                </div>
