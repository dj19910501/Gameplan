                        </span>
                    </div>
                </div>
            </div>
            <div id="dvRevenueToPlan">
                @Html.Partial("_RevenueToPlan", revenue_to_planModel)
            </div>
        </div>
        <div id="dvRevenueCardSection">
            @Html.Partial("_ReportCardSection", revenue_cardsectionModel)
