        $.ajax({
            type: 'POST',
            url: '@Url.Content("~/Report/LoadCampaignDropDownDynamic/")',
            data: {
                marsterCustomField: marsterCustomField,
                masterCustomFieldOptionId: masteroptionid,
                selectOption: optionmain
