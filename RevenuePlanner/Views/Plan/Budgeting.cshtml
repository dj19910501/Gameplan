        $.each(items, function (index, plan) {
            if (plan.Selected) {
                $html += '<option value="' + plan.Value + '" selected>' + plan.Text + '</option>';
                CurrentPlanId = plan.Value;
            } else {
                $html += '<option value="' + plan.Value + '">' + plan.Text + '</option>';
