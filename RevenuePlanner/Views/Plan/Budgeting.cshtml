        // Start - #1942
        var listCheckbox = $("#ulSelectedYear").find("input[type=checkbox]");
        var years = "";
        $.each(listCheckbox, function () {
            if ($(this).attr("checked")) {
                years += $(this).attr('yearValue') + ",";
