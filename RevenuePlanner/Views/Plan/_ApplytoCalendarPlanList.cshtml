@model RevenuePlanner.Models.HomePlan

<ul class="nav nav-tabs">
    <li class="title-header source-sans-proregular bold font-size25px">

        <form id="nl-form2" class="nl-form-plan-title without-margin plantitle" style="display:none">

            <select id="dropdown-plan-title" data-default="0%" class="" maxlength="40">
                <script>
                    var plans = @Html.Raw(Json.Encode(Model.plans))
                    populateNF(plans, "dropdown-plan-title");
                </script>
            </select>
            <div class="nl-overlay"></div>
            <!--<div class="nl-overlay"></div>-->
            <script>
                var nlform = new NLForm(document.getElementById('nl-form2'));
            </script>
            
        </form>
    </li>
    <li class="active pull-right">
        <a id="ATactics" href="#">Tactics</a>
    </li>
</ul>
<script src="~/Scripts/js/scripts.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        onClickPlanTitle();
        if (plans.length > 0) {
            $('#DivloadPlan').show();
            $('.tab-content').show();
            $('#ptacticcount').show();
            $('#chart2').show();
            $('#pbudget').show();
            $('#pMQLs').show();
            $('.changes').show();

            //// Modified By Maninder Singh Wadhva to Address PL#203
            GetCollaborators(CurrentPlanId);
            RefreshCalendar();
            GetHealderData(CurrentPlanId);
            GetNumberOfActivityPerMonByPlanId(CurrentPlanId, "");
            ShowChangeLog();
            $('#Notabcontent').hide();
        }
        else {
            $('#DivloadPlan').hide();
            $('.tab-content').hide();
            $('#ptacticcount').hide();
            $('#chart2').hide();
            $('#pbudget').hide();
            $('#pMQLs').hide();
            $('.changes').hide();
            $('#Notabcontent').show();
        }
    });


</script>

