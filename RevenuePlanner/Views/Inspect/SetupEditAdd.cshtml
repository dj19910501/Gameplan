                        $("#txtmql").val(data.mql);
                        $("#txtmql").attr("title", data.mql);
                        $("#txtmql").removeClass('priceValue');
                    }
                    else {
                        mqlConversion = data.mql;
                        var mqlValue = FormatCommas(mqlConversion.toString(), false);
                        $("#txtmql").val(mqlValue);
                        $("#txtmql").attr("title", mqlValue);
