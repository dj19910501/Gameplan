                type: 'POST',
                url: '@Url.Content("~/Inspect/CalculateMQL")',
                data: $("form").serialize() + '&projectedStageValue=' + projectedStageValue + '&RedirectType=' + isAssortment + '&isTacticTypeChange=' + isTacticTypeChange,
