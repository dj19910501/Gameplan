<div class="modal fade <%= o.css.editor %>">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
                <h4 class="modal-title">Link to Plan</h4>
            </div>
            <div class="modal-body">
                <div class="<%= o.css.existingLinks %>">
                    <h4>
                        <span class="<%= o.css.availableFunds %>"></span>
                        <span class="<%= o.css.availableFundsLabel %>">Available Funds</span>
                        <span class="<%= o.css.transactionName %>"><%- o.transaction.ClientTransactionId %></span>
                        <span class="<%= o.css.totalLabel %>">Total Transaction</span>
                        <span class="<%= o.css.totalTransaction %>"><%- o.currency(o.transaction.Amount) %></span>
                    </h4>
                    <div class="<%= o.css.links %> <%= o.css.loading %>">
                        <i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
                        <strong>Loading...</strong>
                    </div>
                </div>
                <div class="<%= o.css.addNewLink %>">
                    <div class="<%= o.css.label %>">Add New Link</div>
                    <select id="<%= o.selectYearId %>" disabled="disabled"><option>Select Year</option></select>
                    <select id="<%= o.selectPlanId %>" disabled="disabled" class="ui-helper-hidden"><option>Select Plan</option></select>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default <%= o.css.cancel %>" data-dismiss="modal">Close</button>
                <button type="button" disabled="disabled" class="btn btn-primary <%= o.css.save %>">Save</button>
            </div>
        </div>
    </div>
</div>
